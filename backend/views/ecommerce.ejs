    <div class="container">
        <table class="table mt-3 text-secondary">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Product Name</th>
                <th scope="col">Description</th>
                <th scope="col">Price</th>
                <th scope="col">View</th>
              </tr>
            </thead>
            <tbody>
                <% for(object of objects){ %>
                  <% link = '/api/store/productId/'+object._id %>
                    <tr>
                        <th><%= object.id %></th>
                        <th><%= object.name %></th>
                        <th><%= object.description %></th>
                        <th><%= object.price + 'â‚¹' %></th>
                      <th><a href=<%= link %>>See</a></th>
                  </tr>
                <%} %>
            </tbody>
          </table>
    </div>

    <nav aria-label="Page navigation example" class="text-center">
      <ul class="pagination justify-content-center">
        <% if (pagination.hasPrevPage) { %>
          <li class="page-item">
            <a class="page-link" href="?page=1">&laquo; First</a>
          </li>
          <li class="page-item">
            <a class="page-link" href="?page=<%= pagination.prevPage %>">Previous</a>
          </li>
        <% } %>
        <% for (let page_number = 1; page_number <= pagination.totalPages; page_number++) { %>
          <% if (pagination.page === page_number) { %>
            <li class="page-item active">
              <span class="page-link"><%= page_number %></span>
            </li>
          <% } else { %>
            <li class="page-item">
              <a class="page-link" href="?page=<%= page_number %>"><%= page_number %></a>
            </li>
          <% } %>
        <% } %>
        <% if (pagination.hasNextPage) { %>
          <li class="page-item">
            <a class="page-link" href="?page=<%= pagination.nextPage %>">Next</a>
          </li>
          <li class="page-item">
            <a class="page-link" href="?page=<%= pagination.totalPages %>">Last &raquo;</a>
          </li>
        <% } %>
      </ul>
    </nav>
    